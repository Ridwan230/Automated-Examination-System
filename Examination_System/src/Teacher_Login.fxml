<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Teacher_Login_Controller">
    <stylesheets>
           <URL value="@newCascadeStyleSheet.css" />
         </stylesheets>
    
   <children>
      <Label layoutX="94.0" layoutY="72.0" prefHeight="26.0" prefWidth="133.0" text="Teacher Login" textFill="#f4f5ed">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font></Label>
      <Button fx:id="Sign_in" layoutX="14.0" layoutY="216.0" mnemonicParsing="false" onAction="#Sign_in" prefHeight="28.0" prefWidth="126.0" text="SignIn" />
      <TextField fx:id="textfield_username" layoutX="29.0" layoutY="113.0" prefHeight="26.0" prefWidth="243.0" promptText="username">
         <font>
            <Font size="11.0" />
         </font></TextField>
      <PasswordField fx:id="textfield_password" layoutX="29.0" layoutY="156.0" prefHeight="26.0" prefWidth="243.0" promptText="password" />
      <Button layoutX="160.0" layoutY="216.0" mnemonicParsing="false" onAction="#Create_account" prefHeight="26.0" prefWidth="126.0" text="Create Account" />
      <Button layoutX="85.0" layoutY="257.0" mnemonicParsing="false" onAction="#Back" prefHeight="29.0" prefWidth="126.0" text="Back" />
   </children>
</AnchorPane>
